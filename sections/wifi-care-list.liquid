<div class="wifi_care_list relative large-row container" data-section-id="{{ section.id }}" data-section-type="wifi-care-list">
  <div class="page-width">
    {% if section.settings.title != blank %}
      <div class="wifi-full-row">
        <div class="text-left sub-title wifi_care_list_main_sub_title">{{ section.settings.sub-title | escape }}</div>
      </div>
      <div class="large-row wifi-main-title wifi_care_list">
        <h2 class="wifi_care_list_main_title text-left {% if section.settings.enlarge_title %}giant-title{% else %}large-title{% endif%}">{{ section.settings.title | escape }}</h2>
      </div>
    {% endif %}

    <div class="grid-flex">
      {%- assign grid_class = 'one-quarter' -%}
      {%- if section.settings.grid == 2 -%}
        {%- assign grid_class = 'one-half' -%}
      {%- elsif section.settings.grid == 3 -%}
        {%- assign grid_class = 'one-third' -%}
      {%- elsif section.settings.grid == 5 -%}
        {%- assign grid_class = 'one-fifth' -%}
      {%- endif -%}
      {% for block in section.blocks %}

      {% capture grid_class_list %}{{ grid_class }} small-down--one-half{% endcapture %}
			
       

          <div class="care_item_{{ forloop.index }} grid-flex__item {{ grid_class_list }}" {{ block.shopify_attributes }}>
            
            
            
            <a class="care_list_link" href="{% if block.settings.box_link != blank %}{{ block.settings.box_link }}{% else %}#{% endif %}">
            <div class="care-list-holder">
              	<div class="each-care-box">
                  	<div class="care-title-set">
                      <div class="care-title-holder">
                        <div class="wifi-icon-bg" style="background:{{block.settings.box_color}}"></div>
                        <div class="wify-icon-box inline-photo show-on-scroll">
                          <svg width="26" height="18" viewBox="0 0 26 18" fill="none" xmlns="http://www.w3.org/2000/svg">
  <path d="M23.1549 12.0692C22.5671 12.9829 21.8383 13.8455 20.9893 14.5971C19.0718 16.2932 16.8359 17.2265 14.8303 17.2265C14.6041 17.2265 14.3816 17.211 14.1611 17.1875C19.2909 19.7497 25.0312 15.6378 25.9186 13.3365C24.9488 12.7747 24.0269 12.3614 23.1549 12.0692Z" fill="{{block.settings.box_color}}"/>
